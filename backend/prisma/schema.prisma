generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Admin {
  id                String    @id @default(uuid())
  username          String    @unique
  password          String
}

model Pages {
  id    String @id @default(uuid())
  page  String @unique
  data  Json
}